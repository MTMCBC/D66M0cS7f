.live-streaming
  .channels-list
    width 20%
    float right
    margin-top 77px
    .channels
      position: relative;
      width: 100%;
      height: 360px;
      float: left;
      padding-left: 10px;
      padding-right: 10px;
      box-sizing: border-box
      div
        width: 100%;
        height: 60px;
        background-repeat: no-repeat;
        margin-top: 0px;
        background-color: rgb(34,34,34);
        background-size: inherit;
        background-position: center;
  .bottom-carousel
    clear: both
    .ad
      float left
      width: 15%
      background-color green
      height 200px
    hub-items-carousel
      float right
      width 85%

