.hubFooter-main-container
  position: relative;
  height 300px;
  clear both

  .hubFooter-container
    width: 100%;
    height: 300px;
    background-color:#282828;
    bottom 0;
    position: absolute;
    -webkit-box-shadow: 0px 13px 57px 12px rgba(0,0,0,0.75);
    -moz-box-shadow: 0px 13px 57px 12px rgba(0,0,0,0.75);
    box-shadow: 0px 13px 57px 12px rgba(0,0,0,0.75);

  .hubFooter-right{
    float: right;
    width: 23%;
    height: 88%;
    padding: 20px;
    margin-top: 1%;
    color: #fff;
    text-align: right;
    //border-left: solid 2px #fff;
    margin-right : 30px;
  }

  .right-list{
    margin-top: 30px;
  }

  .hubFooter-middle-right{
    float: right;
    width: 20%;
    height: 88%;
    padding: 20px;
    margin-top: 1%;
    color: #fff;
    text-align: right;
    //border-left: solid 2px #fff;
    margin-right : 30px;
  }

  .hubFooter-middle-left{
    float: right;
    width: 20%;
    height: 88%;
    padding: 20px;
    margin-top: 1%;
    color: #fff;
    text-align: right;
    margin-right : 30px;
  }

  .stay-conected-list{
    margin-top: 30px;
  }

  .stay-conected-list div{
    float : right;
    height 40px;
    padding-right : 40px;
    width 100%;
  }

  .channels{
    clear:both;
  }

  .channels-list{
    float : right;
  }

  .channels-list div{
    width: 100px;
    height: 55px;
    float: right;
    background-repeat: no-repeat;
    margin-right: 20px;
    margin-top: 4px;
  }

  .hubFooter-left{
    float: right;
    width: 20%;
    height: 92%;
    padding: 20px;
    margin-top: 1%;
    color: #fff;
    text-align: right;
    margin-right : 20px;
    }

    .left-list div{
      margin-right: 20px;
      padding-top : 20px;
    }

  .stay-conected {
    font-weight: bold;
    font-size: 19px;
  }


  .channels {
    font-weight: bold;
    font-size: 19px;
  }


  .extra-info {
    font-weight: bold;
    font-size: 19px;
  }

  .fb-logo{
      background-image :url("/assets/images/footer-facebook.png")
    }

  .twitter-logo{
    background-image :url("/assets/images/footer-twitter.png")
  }


  .plus-logo{
    background-image :url("/assets/images/footer-googleplus.png")
  }

  .footer-social-logo{
    width: 30px;
    height: 30px;
    float: left !important;
    margin-top: -6px;
    margin-right: 5px;
    margin-left: 10px;
    background-repeat : no-repeat;
    position absolute
  }

   .footer-footer{
     background-color: #1a1a1b;
     height :50px;
     clear :both;
     color :white;
     padding-right :10%;
     padding-top: 13px;

   }
  .footer-footer div{
    float :right;
    font-size:12px;
  }
